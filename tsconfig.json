{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs", /* Disable generating custom helper functions like '__extends' in compiled output. */ /* Ensure that each file can be safely transpiled without relying on other imports. */
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true, /* Ensure that casing is correct in imports. */
        "strict": true,
        "skipLibCheck": true
    }
}